{
    "name": "Xilinx TRTRS",
    "description": "Linear System Solver for dense upper / lower triangular matrix",
    "flow": "system",
    "platform_allowlist": [
        "u250",
        "vck190",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/kernel_trtrs.xclbin",
            "name": "generic launch for all flows"
        }
    ],
    "host": {
        "host_exe": "test_trtrs.exe",
        "compiler": {
            "sources": [
                "test_trtrs.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/ext/MatrixGen",
                "LIB_DIR/../utils/L1/include"
            ],
            "options": "-O3 "
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/ext"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel_trtrs.cpp",
                    "frequency": 200.0,
                    "clflags": " -D KERNEL_NAME=kernel_trtrs_0",
                    "name": "kernel_trtrs_0",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "kernel_trtrs_0_1",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "dataA",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "dataB",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 200.0,
            "name": "kernel_trtrs"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 120,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    },
    "gui": false
}