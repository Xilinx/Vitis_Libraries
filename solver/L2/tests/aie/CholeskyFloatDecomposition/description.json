{
    "name": "Cholesky Float on AIE",
    "description": "Cholesky Float on AIE",
    "flow": "system",
    "platform_allowlist": [
        "vck190"
    ],
    "platform_blocklist": [
        "vek280"
    ],
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include",
                "LIB_DIR/L1/include/aie",
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/include/aie",
                "LIB_DIR/L1/src",
                "LIB_DIR/L1/src/aie",
                "./"
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/aie/platform.cpp",
                    "files": [
                        "LIB_DIR/L1/src/aie/cholesky_float_decomposition.cpp"
                    ],
                    "workdir": "./Work",
                    "name": "libadf.a",
                    "config": "aie.cfg"
                }
            ]
        }
    ],
    "data": [
        "PROJECT/data/matA_64.txt",
        "PROJECT/data/gldMatL_64.txt"
    ],
    "launch": [
        {
            "cmd_args": "BUILD/test_harness.xclbin PROJECT/data/matA_64.txt PROJECT/data/gldMatL_64.txt",
            "name": "generic launch for all flows"
        },
        {
            "name": "aiesim-launch",
            "target": "aiesim",
            "sim_options": "--input-dir=./ --output-dir=./"
        },
        {
            "name": "x86sim-launch",
            "target": "x86sim",
            "sim_options": "--input-dir=./ --output-dir=./"
        }
    ],
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/aie/CholeskyFloatDecomposition/ps/host.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/tests/aie/CholeskyFloatDecomposition/ps",
                "LIB_DIR/L2/tests/aie/CholeskyFloatDecomposition/aie",
                "$(SYSROOT)/usr/include/xrt/",
                "LIB_DIR/L1/include/aie",
                "LIB_DIR/L2/include/aie"
            ],
            "symbols": [
                "__PS_ENABLE_AIE__"
            ]
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                ".pl"
            ]
        },
        "linker": {
            "ldclflags": [
                "--config PROJECT/cfg/system.cfg"
            ]
        },
        "package": {
            "clflags": [
                "--package.defer_aie_run"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "name": "vck190_test_harness",
                    "location": "./pl/test_harness.cpp",
                    "frequency": 250.0
                }
            ],
            "name": "test_harness",
            "frequency": 250.0
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": 40960,
                "max_time_min": {
                    "vitis_aie_sim": 480,
                    "vitis_aie_x86sim": 200,
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 200,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
            "vitis_aie_sim",
            "vitis_aie_x86sim"
        ],
        "category": "canary"
    },
    "gui": true,
    "aie_types": [
        "AIE1"
    ]
}
