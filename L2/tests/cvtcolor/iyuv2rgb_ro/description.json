{
    "name": "Xilinx Iyuv2Rgb_Ro L2 Test", 
    "description": [
        "Cvtcolor L2 function example"
    ], 
    "flow": "vitis", 
    "platform_whitelist": [
        "u200", 
        "zcu102", 
        "zcu104"
    ], 
    "platform_blacklist": [
        "u280", 
        "u250", 
        "u50"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(OPENCV_INCLUDE)"
                    ]
                }, 
                "linker": {
                    "librarypaths": [
                        "$(OPENCV_LIB)"
                    ]
                }
            }
        }, 
        "zcu102": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include"
                    ], 
                    "options": "--sysroot=$(SYSROOT)"
                }, 
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib", 
                        "${SYSROOT}/opt/xilinx/xrt/lib"
                    ]
                }
            }
        }, 
        "zcu104": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include"
                    ], 
                    "options": "--sysroot=$(SYSROOT)"
                }, 
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib", 
                        "${SYSROOT}/opt/xilinx/xrt/lib"
                    ]
                }
            }
        }
    }, 
    "gui": false, 
    "launch": [
        {
            "target": "sw_emu", 
            "cmd_args": "LIB_DIR/data/input.jpg LIB_DIR/data/ref_Y.png LIB_DIR/data/ref_U.png LIB_DIR/data/ref_V.png", 
            "name": "sw_emu", 
            "ld_library_path": [
                "$(OPENCV_LIB)", 
                "$(LD_LIBRARY_PATH)", 
                "$(XILINX_XRT)/lib"
            ]
        }, 
        {
            "target": "hw_emu", 
            "cmd_args": "LIB_DIR/data/input.jpg LIB_DIR/data/ref_Y.png LIB_DIR/data/ref_U.png LIB_DIR/data/ref_V.png", 
            "name": "hw_emu", 
            "ld_library_path": [
                "$(OPENCV_LIB)", 
                "$(LD_LIBRARY_PATH)", 
                "$(XILINX_XRT)/lib"
            ]
        }, 
        {
            "target": "hw", 
            "cmd_args": "LIB_DIR/data/input.jpg LIB_DIR/data/ref_Y.png LIB_DIR/data/ref_U.png LIB_DIR/data/ref_V.png", 
            "name": "hardware_build", 
            "ld_library_path": [
                "$(OPENCV_LIB)", 
                "$(LD_LIBRARY_PATH)", 
                "$(XILINX_XRT)/lib"
            ]
        }
    ], 
    "data": [
        "LIB_DIR/data/input.jpg", 
        "LIB_DIR/data/ref_Y.png", 
        "LIB_DIR/data/ref_U.png", 
        "LIB_DIR/data/ref_V.png"
    ], 
    "host": {
        "host_exe": "cvtcolor", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/examples/cvtcolor/xf_cvt_color_tb.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "options": "-O3", 
            "symbols": [
                "__SDSVHLS__"
            ]
        }
    }, 
    "v++": {
        "compiler": {
            "symbols": [], 
            "clflags": []
        }
    }, 
    "containers": [
        {
            "name": "krnl_cvt_color", 
            "accelerators": [
                {
                    "name": "cvtcolor_rgba2iyuv", 
                    "location": "LIB_DIR/L2/examples/cvtcolor/xf_cvt_color_accel.cpp"
                }
            ]
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 470, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 30
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "full"
    }
}