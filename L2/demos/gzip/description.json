{
    "name": "Xilinx GZIP Compression and Decompression", 
    "description": [
        "GZip Compression and Decompression"
    ], 
    "flow": "vitis", 
    "launch": [
        {
            "cmd_args": "-sx BUILD/compress_decompress.xclbin -v PROJECT/sample.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "post_launch": [
        {
            "launch_cmd": [
                  "./run.sh HOST_EXE LIB_DIR BUILD/compress_decompress.xclbin"
             ]
        }
    ], 
    "platform_blacklist": [
        "zc", 
        "vck",
        "u50"
    ], 
    "platform_whitelist": [
        "u200", 
        "u250"
    ], 
    "host": {
        "host_exe": "xil_gzip", 
        "compiler": {
            "sources": [
                "./src/host.cpp", 
                "LIB_DIR/L2/tests/src/zlib.cpp", 
                "LIB_DIR/common/libs/xcl2/xcl2.cpp", 
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp", 
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/zlib-1.2.7/crc32.c" 
            ], 
            "includepaths": [
                "LIB_DIR/L2/tests/src/", 
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/common/thirdParty/zlib-1.2.7"
            ], 
            "symbols": [
                "PARALLEL_BLOCK=8", 
                "GZIP_MODE=1"
            ]
        }
    }, 
    "v++": {
        "build_datafiles": [
            "PROJECT/advanced.cfg", 
            "PROJECT/hls_directives.tcl"
        ], 
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=8", 
                "MULTIPLE_BYTES=4", 
                "GZIP_MODE=1"
            ], 
            "clflags": [
                "--config PROJECT/advanced.cfg"
            ]
        }
    }, 
    "containers": [
        {
            "name": "compress_decompress", 
            "ldclflags": "--config PROJECT/opts.cfg", 
            "accelerators": [
                {
                    "name": "xilLz77Compress", 
                    "location": "LIB_DIR/L2/src/zlib_lz77_compress_mm.cpp"
                }, 
                {
                    "name": "xilHuffmanKernel", 
                    "location": "LIB_DIR/L2/src/zlib_huffman_enc_mm.cpp"
                }, 
                {
                    "name": "xilZlibDmReader", 
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_rd.cpp"
                }, 
                {
                    "name": "xilZlibDmWriter", 
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_wr.cpp"
                }, 
                {
                    "name": "xilDecompressFull", 
                    "location": "LIB_DIR/L2/src/zlib_parallelbyte_decompress_full.cpp"
                }
            ]
        }
    ], 
    "output_files": "sample.txt.* sample_run.* test.list", 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 25, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
