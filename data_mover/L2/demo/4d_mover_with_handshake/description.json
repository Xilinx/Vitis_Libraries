{
    "name": "Generated 4D Data Mover with HW handshake Test",
    "description": [
        "Generated configurable 4D Read/Write data mover kernel with handshake, using parameters."
    ],
    "gui": false,
    "flow": "system",
    "platform_allowlist": [
        "vck190",
        "u200"
    ],
    "runtime": [
        "OpenCL"
    ],
    "host": {
        "host_exe": "test.exe",
        "compiler": {
            "sources": [
                "sw/test.cpp"
            ],
            "includepaths": [
                "sw"
            ],
            "options": "-std=c++17 -g"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include",
                "LIB_DIR/L2/include"
            ],
            "clflags": [
                "--hls.pre_tcl pl/runPre.tcl"
            ]
        },
        "linker": {
            "ldclflags": [
                "-g --config PROJECT/system.cfg"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "pl/mm2s_4d_hsk.cpp",
                    "frequency": 300,
                    "name": "mm2s_4d_hsk"
                },
                {
                    "location": "pl/s2mm_mp.cpp",
                    "frequency": 300,
                    "name": "s2mm_mp"
                },
                {
                    "location": "pl/s2mm_4d_hsk.cpp",
                    "frequency": 300,
                    "name": "s2mm_4d_hsk"
                },
                {
                    "location": "pl/mm2s_mp.cpp",
                    "frequency": 300,
                    "name": "mm2s_mp"
                }
            ],
            "frequency": 250,
            "name": "data_mover"
        }
    ],
    "generators": {
        "pl/mm2s_4d_hsk.cpp": {
            "spec": "LIB_DIR/L2/meta/mm2s_4d_hsk.json",
            "api_name": "xf::data_mover::mm2s_4d_hsk"
        },
        "pl/s2mm_mp.cpp": {
            "spec": "LIB_DIR/L2/meta/s2mm_mp.json",
            "api_name": "xf::data_mover::s2mm_mp"
        },
        "pl/s2mm_4d_hsk.cpp": {
            "spec": "LIB_DIR/L2/meta/s2mm_4d_hsk.json",
            "api_name": "xf::data_mover::s2mm_4d_hsk"
        },
        "pl/mm2s_mp.cpp": {
            "spec": "LIB_DIR/L2/meta/mm2s_mp.json",
            "api_name": "xf::data_mover::mm2s_mp"
        },
        "system.cfg": {
            "file": "PROJECT/test_gen.py",
            "function": "write_system_cfg"
        }
    },
    "parameter_file": "config_params.json",
    "launch": [
        {
            "name": "generic launch for all flows",
            "cmd_args": "BUILD/data_mover.xclbin"
        }
    ],
    "testinfo": {
        "disable": true,
        "stacksize": 16384,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60
                }
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw_build"
        ],
        "category": "canary"
    }
}
