{
    "gui": false,
    "name": "Xilinx Jacobi Singular Value Decomposition Benchmark",
    "description": "The singular value decomposition (SVD) is a very useful technique for dealing with general dense matrix problems.",
    "flow": "vitis",
    "platform_type": "pcie",
    "platform_allowlist": [
        "u250",
        "u200"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            },
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u200.cfg"
                    ]
                }
            }
        },
        "u250": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            },
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        },
        "u50": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            },
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/kernel_svd.xclbin",
            "name": "generic launch for all flows",
            "ld_library_path": [
                "$(XILINX_HLS)/lnx64/tools/fpo_v7_1"
            ]
        }
    ],
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "host/main.cpp",
                "host/svd.cpp",
                "host/util.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/L1/benchmarks/SVD/kernel",
                "LIB_DIR/L1/include",
                "LIB_DIR/L1/benchmarks/SVD/host"
            ],
            "options": "-O3  -I$(XILINX_HLS)/include"
        },
        "linker": {
            "options": " -L$(XILINX_HLS)/lnx64/tools/fpo_v7_1 -lgmp -lmpfr -lIp_floating_point_v7_1_bitacc_cmodel"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include",
                "LIB_DIR/L1/benchmarks/SVD/kernel"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/kernel_svd.cpp",
                    "frequency": 300.0,
                    "name": "kernel_svd_0",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "kernel_svd_0_1",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "dataA",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "sigma",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "dataU",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "dataV",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 300.0,
            "name": "kernel_svd"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 360,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}