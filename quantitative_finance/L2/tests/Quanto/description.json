{
    "name": "Xilinx Quanto Options Model",
    "description": [
        "L2 Quanto Options Model"
    ],
    "flow": "system",
    "gui": false,
    "platform_type": "pcie",
    "platform_allowlist": [
        "u250"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }
    },
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 61400,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    },
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "LIB_DIR/ext/xcl2/xcl2.cpp",
                "./src/host/quanto_cpu.cpp",
                "./src/host/quanto_parser.cpp",
                "./src/host/quanto_test.cpp"
            ],
            "options": " -I$(XILINX_HLS)/include"
        },
        "linker": {
            "options": " -L$(XILINX_HLS)/lnx64/tools/fpo_v7_1 -lgmp -lmpfr -lIp_floating_point_v7_1_bitacc_cmodel"
        }
    },
    "containers": [
        {
            "name": "quanto_kernel",
            "frequency": 200.0,
            "accelerators": [
                {
                    "name": "quanto_kernel",
                    "location": "src/kernel/quanto_kernel.cpp",
                    "frequency": 200.0
                }
            ]
        }
    ],
    "launch": [
        {
            "name": "generic launch for all flows",
            "cmd_args": "BUILD/quanto_kernel.xclbin PROJECT/one.txt exp",
            "ld_library_path": [
                "$(XILINX_HLS)/lnx64/lib/csim",
                "$(XILINX_HLS)/lnx64/tools/fpo_v7_1"
            ]
        }
    ]
}