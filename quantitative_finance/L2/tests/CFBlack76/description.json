{
    "name": "Xilinx Closed Form Black 76 Model",
    "description": "L2 Closed Form Black 76 Model",
    "flow": "system",
    "platform_type": "pcie",
    "platform_allowlist": [
        "u250"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " BUILD/b76_kernel.xclbin 16384",
            "name": "generic launch for all flows",
            "ld_library_path": [
                "$(XILINX_HLS)/lnx64/lib/csim",
                "$(XILINX_HLS)/lnx64/tools/fpo_v7_1"
            ]
        }
    ],
    "host": {
        "host_exe": "b76_test.exe",
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/CFBlack76/src/host/b76_test.cpp",
                "LIB_DIR/L2/tests/CFBlack76/src/host/b76_model.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/ext/xcl2/"
            ],
            "options": "-O3  -I$(XILINX_HLS)/include"
        },
        "linker": {
            "options": " -L$(XILINX_HLS)/lnx64/tools/fpo_v7_1 -lgmp -lmpfr -lIp_floating_point_v7_1_bitacc_cmodel"
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/CFBlack76/src/kernel/b76_kernel.cpp",
                    "frequency": 300.0,
                    "name": "b76_kernel"
                }
            ],
            "frequency": 300.0,
            "name": "b76_kernel"
        }
    ],
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include"
            ]
        }
    },
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 51200,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    },
    "gui": true
}