{
    "name": "Xilinx Heston Closed Form Engine",
    "description": "L2 Heston Closed Form Solution",
    "flow": "system",
    "gui": false,
    "platform_type": "pcie",
    "platform_allowlist": [
        "u250"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR",
                        "TEST_DT=float",
                        "DEVICE_PART=u250"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -f PROJECT/utils/one.txt -v -c -b BUILD/hcf.xclbin",
            "name": "generic launch for all flows",
            "ld_library_path": [
                "$(XILINX_HLS)/lnx64/lib/csim",
                "$(XILINX_HLS)/lnx64/tools/fpo_v7_1"
            ]
        }
    ],
    "host": {
        "host_exe": "hcf_host.exe",
        "compiler": {
            "sources": [
                "./src/host/hcf_host.cpp",
                "./src/host/hcf_cpu.cpp",
                "./src/host/hcf_parser.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L1/include",
                "LIB_DIR/L2/include",
                "./src/kernel",
                "LIB_DIR/ext/xcl2"
            ],
            "options": "-O3  -I$(XILINX_HLS)/include",
            "symbols": [
                "TEST_DT=float",
                "DEVICE_PART=u250"
            ]
        },
        "linker": {
            "options": " -L$(XILINX_HLS)/lnx64/tools/fpo_v7_1 -lgmp -lmpfr -lIp_floating_point_v7_1_bitacc_cmodel"
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/HCFEngine/src/kernel/hcf_kernel.cpp",
                    "frequency": 300.0,
                    "clflags": " -D TEST_DT=float",
                    "name": "hcf_kernel"
                }
            ],
            "frequency": 300.0,
            "name": "hcf"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 720,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}