{
    "gui": false,
    "name": "Xilinx Credit Default Swap Model",
    "description": [
        "Credit Default Swap Model using the L3 framework"
    ],
    "flow": "vitis",
    "platform_type": "pcie",
    "platform_allowlist": [
        "u250",
        "u200",
        "u50"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "host": {
        "host_exe": "cds_test.exe",
        "compiler": {
            "sources": [
                "LIB_DIR/ext/xcl2/xcl2.cpp",
                "LIB_DIR/L3/tests/cds/xf_fintech_cds_exe.cpp",
                "LIB_DIR/L3/src/models/cds/src/xf_fintech_cds.cpp",
                "LIB_DIR/L3/src/xf_fintech_device.cpp",
                "LIB_DIR/L3/src/xf_fintech_device_manager.cpp",
                "LIB_DIR/L3/src/xf_fintech_internal.cpp",
                "LIB_DIR/L3/src/xf_fintech_ocl_controller.cpp",
                "LIB_DIR/L3/src/xf_fintech_timestamp.cpp",
                "LIB_DIR/L3/src/xf_fintech_trace.cpp"
            ],
            "includepaths": [
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/L3/include",
                "LIB_DIR/L3/include/models",
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/kernel",
                "LIB_DIR/L3/src/models/cds/include",
                "LIB_DIR/L1/include",
                "LIB_DIR/L2/include"
            ],
            "options": "-O3  -I$(XILINX_HLS)/include"
        },
        "linker": {
            "options": " -L$(XILINX_HLS)/lnx64/tools/fpo_v7_1 -lgmp -lmpfr -lIp_floating_point_v7_1_bitacc_cmodel"
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/kernel/CDS_kernel.cpp",
                    "frequency": 300.0,
                    "clflags": " -D TEST_DT=float",
                    "name": "CDS_kernel"
                }
            ],
            "frequency": 300.0,
            "name": "CDS_kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include",
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/kernel"
            ]
        }
    },
    "launch": [
        {
            "name": "generic launch for all flows",
            "platform": "u250",
            "cmd_args": "BUILD/CDS_kernel.xclbin u250",
            "ld_library_path": [
                "$(XILINX_HLS)/lnx64/tools/fpo_v7_1"
            ]
        },
        {
            "name": "generic launch for all flows",
            "platform": "u200",
            "cmd_args": "BUILD/CDS_kernel.xclbin u200"
        },
        {
            "name": "generic launch for all flows",
            "platform": "u50",
            "cmd_args": "BUILD/CDS_kernel.xclbin u50"
        }
    ]
}