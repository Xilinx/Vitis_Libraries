{
    "name": "Xilinx Pre-Process AIE-ML Test", 
    "description": "Pre-Process AIE-ML function example", 
    "flow": "system", 
    "platform_allowlist": [
        "vek280",
        "vck190",
        "v70"
    ], 
    "platform_blocklist": [
        "vck5000"
    ], 
    "platform_properties": {
        "vek280": {
            "v++": {
                "compiler": {
                    "clflags": [
                        ""
                    ]
                }, 
                "linker": {
                    "ldclflags": [
                        "--clock.freqHz 400000000:Tiler_top_1", 
                        "--clock.freqHz 400000000:stitcher_top_1", 
                        "--clock.defaultTolerance 0.001", 
                        "--config PROJECT/system.cfg"
                    ]
                }, 
                "package": {
                    "clflags": [
                        "--package.defer_aie_run"
                    ]
                }
},
			"host": {
				"compiler" : {
			   	"includepaths": [
						"$(SYSROOT)/usr/include/xrt/", 
						"$(SYSROOT)/usr/include/", 
						"$(SYSROOT)/usr/include/opencv4" 
                	], 
            		"options": "", 
            		"symbols": [
                		"__PS_ENABLE_AIE__"
            			]
				},
        		"linker": {
            			"librarypaths": [
		                	"$(SYSROOT)/usr/lib/", 
                			"LIB_DIR/L1/lib/sw/aarch64-linux"
            			], 
            		"options": " -luuid"
                }
            }
        },
        "vck190": {
            "v++": {
                "compiler": {
                    "clflags": [
                        ""
                    ]
                }, 
                "linker": {
                    "ldclflags": [
                        "--clock.freqHz 400000000:Tiler_top_1", 
                        "--clock.freqHz 400000000:stitcher_top_1", 
                        "--clock.defaultTolerance 0.001", 
                        "--config PROJECT/system.cfg"
                    ]
                }, 
                "package": {
                    "clflags": [
                        "--package.defer_aie_run"
                    ]
                }
},
			"host": {
				"compiler": {
					"includepaths": [
						"$(SYSROOT)/usr/include/xrt/", 
						"$(SYSROOT)/usr/include/", 
						"$(SYSROOT)/usr/include/opencv4" 
                	], 
            		"options": " ", 
            		"symbols": [
                		"__PS_ENABLE_AIE__"
            			]
				},
        		"linker": {
            			"librarypaths": [
		                	"$(SYSROOT)/usr/lib/", 
                			"LIB_DIR/L1/lib/sw/aarch64-linux"
            			], 
            		"options": "-luuid"
        		}
            }
        },
        "v70": {
            "v++": {
                "compiler": {
					"clflags": [
								""
					]
				},
				"linker": {
                	"ldclflags": [
                        	"--clock.freqHz 400000000:Tiler_top_1", 
                        	"--clock.freqHz 400000000:stitcher_top_1", 
                        	"--clock.defaultTolerance 0.001", 
                        	"--config PROJECT/system.cfg"
                    	]
				}, 
                "package": {
                    "clflags": [
							""
                    ]
                }
			},
			"host": {
				"compiler":{
					"includepaths": [
						"$(OPENCV_INCLUDE)"
                	], 
            		"options": " ", 
            		"symbols": [
                		"__PS_ENABLE_AIE__"
            			]
				},
        		"linker": {
            			"librarypaths": [
                			"LIB_DIR/L1/lib/sw/x86",
							"$(OPENCV_LIB)"
            			], 
            		"options": "-luuid"
                }
            }
        }
    }, 
    "data": [
        "LIB_DIR/data/4k.jpg", 
        "LIB_DIR/data/HD.jpg", 
        "LIB_DIR/data/512x512.png"
    ], 
    "gui": false, 
    "launch": [
{
            "name": "aiesim-launch",
            "target": "aiesim"
        },
        {
            "name": "x86sim-launch",
            "target": "x86sim"
        },
        {
            "name": "hwemu-launch", 
            "target": "hw_emu", 
            "cmd_args": "kernel.xclbin 512x512.png"
        }, 
        {
            "name": "hw", 
            "target": "hw", 
            "cmd_args": "kernel.xclbin 4k.jpg"
        }
    ], 
"v++": { 
        "compiler": {
            "symbols": [],
            "clflags": [],
			"includepaths": [
				"LIB_DIR/L1/include/aie",
				"LIB_DIR/L1/include/aie-ml", 
				"LIB_DIR/L1/include/aie-ml/dnn", 
				"./"
			]
		},
		"linker": {
               	"ldclflags": [
                       	"LIB_DIR/L1/lib/hw/tiler.xo", 
                       	"LIB_DIR/L1/lib/hw/stitcher.xo"
                   	]
        }
    },
    "host": {
        "host_exe": "host.elf", 
        "compiler": {
            "sources": [
                "LIB_DIR/L3/tests/preprocess/8bit_aie_8bit_pl/host.cpp", 
                "./Work/ps/c_rts/aie_control_xrt.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include/aie",
				"LIB_DIR/L1/include/aie-ml", 
				"LIB_DIR/L1/include/aie-ml/dnn", 
				"./"
            ], 
            "options": "", 
            "symbols": [
                "__PS_ENABLE_AIE__"
            ]
        }, 
        "linker": {
            "librarypaths": [
                "$(OPENCV_LIB)"
            ], 
            "libraries": [
                "smartTilerStitcher"
            ], 
            "options": "-luuid"
        }
    }, 
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/aie",
				"LIB_DIR/L1/include/aie-ml",
                "LIB_DIR/L1/include/aie-ml/dnn"
            ]
        }
    }, 
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L3/tests/preprocess/8bit_aie_8bit_pl/graph.cpp", 
                    "pl-freq": 400, 
                    "workdir": "./Work", 
                    "options": "", 
                    "name": "libadf.a", 
                    "files": [
                        "xf_resize_normalize.cc",
                        "xf_yuv2rgba.cc"
                    ]
                }
            ]
        }
    ], 
    "containers": [
        {
            "frequency": "400", 
            "name": "kernel",
			"ldclflags": "LIB_DIR/L1/lib/hw/tiler.xo LIB_DIR/L1/lib/hw/stitcher.xo"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": {
                    "vitis_aie_sim": 50, 
                    "vitis_aie_x86sim": 50, 
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 200, 
                    "vitis_hw_run": 30
                }
            }
        ], 
        "targets": [
"vitis_aie_sim",
            "vitis_aie_x86sim",
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    },
    "output_files": [
        "pl_sample_counts",
        "ISS_RPC_SERVER_PORT"
    ]
}
