{
    "name": "xilinx bpc l2 test",
    "description": [
        "BPC L2 function example"
    ],
    "flow": "system",
    "platform_allowlist": [
        "zcu104",
        "vck190",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "u280",
        "u250",
        "u50"
    ],
    "platform_properties": {
        "aws-vu9p-f1": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(OPENCV_INCLUDE)"
                    ]
                },
                "linker": {
                    "librarypaths": [
                        "$(OPENCV_LIB)"
                    ]
                }
            }
        },
        "vck190": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include",
                        "$(SYSROOT)/usr/include/opencv4"
                    ]
                },
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib",
                        "${SYSROOT}/lib"
                    ]
                }
            }
        },
        "zcu104": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include",
                        "$(SYSROOT)/usr/include/opencv4"
                    ]
                },
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib",
                        "${SYSROOT}/lib"
                    ]
                }
            }
        }
    },
    "gui": true,
    "launch": [
        {
            "target": "hw_emu",
            "cmd_args": " LIB_DIR/data/hd.png",
            "name": "hw_emu",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        },
        {
            "target": "hw",
            "cmd_args": " LIB_DIR/data/hd.png",
            "name": "hardware_build",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        }
    ],
    "data": [
        "LIB_DIR/data/hd.png",
        "LIB_DIR/data/hd.png"
    ],
    "host": {
        "host_exe": "badpixelcorrection",
        "compiler": {
            "sources": [
                "LIB_DIR/L2/examples/badpixelcorrection/xf_badpixelcorrection_tb.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/tests/badpixelcorrection/badpixelcorrection_NPPC8_8UC1_8UC1"
            ],
            "options": "-O3 -I$(XILINX_HLS)/include -I$(XILINX_HLS)/include -I$(XILINX_HLS)/include",
            "symbols": [
                "__SDSVHLS__"
            ]
        }
    },
    "v++": {
        "compiler": {
            "symbols": [],
            "includepaths": [
                "LIB_DIR/L2/tests/badpixelcorrection/badpixelcorrection_NPPC8_8UC1_8UC1"
            ],
            "clflags": []
        }
    },
    "containers": [
        {
            "name": "krnl_badpixelcorrection",
            "accelerators": [
                {
                    "name": "badpixelcorrection_accel",
                    "location": "LIB_DIR/L2/examples/badpixelcorrection/xf_badpixelcorrection_accel.cpp",
                    "frequency": 300.0
                }
            ],
            "frequency": 300.0
        }
    ],
    "check_env": [
        {
            "env": "OPENCV_INCLUDE",
            "platform": [
                "u200",
                "u50",
                "aws-vu9p-f1"
            ],
            "echo": "Cannot find OpenCV include path. Please set OPENCV_INCLUDE variable"
        },
        {
            "env": "OPENCV_LIB",
            "platform": [
                "u200",
                "u50",
                "aws-vu9p-f1"
            ],
            "echo": "Cannot find Opencv lib path. Please set OPENCV_LIB variable"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 65535,
                "max_time_min": 520
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "full"
    }
}