{
    "name": "xilinx isp stats bayer l2 test",
    "description": [
        "ISP Stats RGB L2 function example"
    ],
    "flow": "system",
    "platform_allowlist": [
        "u200",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "vck190",
        "u280",
        "u250",
        "zcu104"
    ],
    "platform_properties": {
        "aws-vu9p-f1": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(OPENCV_INCLUDE)"
                    ]
                },
                "linker": {
                    "librarypaths": [
                        "$(OPENCV_LIB)"
                    ]
                }
            }
        }
    },
    "gui": true,
    "launch": [
        {
            "target": "sw_emu",
            "cmd_args": " LIB_DIR/data/ltm_input_HD.png LIB_DIR/data/HD_bayer_16bit.png",
            "name": "sw_emu",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        },
        {
            "target": "hw_emu",
            "cmd_args": " LIB_DIR/data/ltm_input_HD.png LIB_DIR/data/HD_bayer_16bit.png",
            "name": "hw_emu",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        },
        {
            "target": "hw",
            "cmd_args": " LIB_DIR/data/ltm_input_HD.png LIB_DIR/data/HD_bayer_16bit.png",
            "name": "hardware_build",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        }
    ],
    "data": [
        "LIB_DIR/data/ltm_input_HD.png",
        "LIB_DIR/data/HD_bayer_16bit.png",
        "LIB_DIR/data/128x128.png",
        "LIB_DIR/data/128x128_bayer.png"
    ],
    "host": {
        "host_exe": "ispstats",
        "compiler": {
            "sources": [
                "LIB_DIR/L2/examples/ispstats/xf_ispstats_tb.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/tests/ispstats/ispstats_NPPC1_16UC1_16UC1_ISPSTATS_BAYER"
            ],
            "options": "-O3 -I$(XILINX_HLS)/include -I$(XILINX_HLS)/include -I$(XILINX_HLS)/include"
        }
    },
    "v++": {
        "compiler": {
            "symbols": [],
            "includepaths": [
                "LIB_DIR/L2/tests/ispstats/ispstats_NPPC1_16UC1_16UC1_ISPSTATS_BAYER"
            ],
            "clflags": []
        }
    },
    "containers": [
        {
            "name": "krnl_ispstats",
            "accelerators": [
                {
                    "name": "ispstats_accel",
                    "location": "LIB_DIR/L2/examples/ispstats/xf_ispstats_accel.cpp",
                    "frequency": 150.0
                }
            ],
            "frequency": 150.0
        }
    ],
    "check_env": [
        {
            "env": "OPENCV_INCLUDE",
            "platform": [
                "u200",
                "u50",
                "aws-vu9p-f1"
            ],
            "echo": "Cannot find OpenCV include path. Please set OPENCV_INCLUDE variable"
        },
        {
            "env": "OPENCV_LIB",
            "platform": [
                "u200",
                "u50",
                "aws-vu9p-f1"
            ],
            "echo": "Cannot find Opencv lib path. Please set OPENCV_LIB variable"
        },
        {
            "env": "EDGE_COMMON_SW",
            "platform": [
                "zcu102",
                "vck190"
            ],
            "echo": "Please set EDGE_COMMON_SW variable"
        }
    ],
    "testinfo": {
        "disable": false,
        "stacksize": 16384,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "full"
    }
}
