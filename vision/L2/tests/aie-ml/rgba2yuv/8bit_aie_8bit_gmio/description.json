{
    "name": "Xilinx rgba2yuv AIE GMIO Test", 
    "description": "rgba2yuv AIE GMIO function example", 
    "flow": "system", 
    "platform_allowlist": [
        "vek280"
    ], 
    "platform_blocklist": [
        "vck190",
        "vck5000",
        "v70"
    ], 
    "platform_properties": {
        "vek280": {
            "v++": {
                "compiler": {
                    "clflags": [
                        ""
                    ]
                }, 
                "linker": {
                    "ldclflags": [
                        "--config PROJECT/system.cfg"
                    ]
                }, 
                "package": {
                    "clflags": [
                        "--package.defer_aie_run"
                    ]
                }
            },
			"host": {
				"compiler" : {
			   	"includepaths": [
						"$(SYSROOT)/usr/include/xrt/", 
						"$(SYSROOT)/usr/include/", 
						"$(SYSROOT)/usr/include/opencv4" 
                	], 
            		"options": "", 
            		"symbols": [
                		"__PS_ENABLE_AIE__"
            			]
				},
        		"linker": {
            			"librarypaths": [
		                	"$(SYSROOT)/usr/lib/", 
                			"LIB_DIR/L1/lib/sw/aarch64-linux"
            			], 
            		"options": " -luuid"
                }
            }
        }
    }, 
    "data": [
        "LIB_DIR/data/cvtcolor/RGBA2IYUV/input/input.jpg" 
    ], 
    "gui": false, 
    "launch": [
{
            "name": "aiesim-launch",
            "target": "aiesim"
        },
        {
            "name": "x86sim-launch",
            "target": "x86sim"
        },
        {
            "name": "hwemu-launch", 
            "target": "hw_emu", 
            "cmd_args": "kernel.xclbin LIB_DIR/data/cvtcolor/RGBA2IYUV/input/input.jpg"
        }, 
        {
            "name": "hw", 
            "target": "hw", 
            "cmd_args": "kernel.xclbin LIB_DIR/cvtcolor/RGBA2IYUV/input/input.jpg"
        }
    ], 
    "v++": { 
            "compiler": {
                "symbols": [],
                "clflags": [],
                "includepaths": [
                    "LIB_DIR/L1/include/aie",
                    "LIB_DIR/L1/include/aie-ml", 
                    "LIB_DIR/L1/include/aie-ml/dnn", 
                    "./"
			]
		},
		"linker": {
               	"ldclflags": [
                   	]
        }
    },
    "host": {
        "host_exe": "host.elf", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/aie-ml/rgba2yuv/8bit_aie_8bit_gmio/host.cpp", 
                "./Work/ps/c_rts/aie_control_xrt.cpp"
            ], 
            "includepaths": [
                                "LIB_DIR/L1/include/aie",
                                "./"
            ], 
            "options": "", 
            "symbols": [
                "__PS_ENABLE_AIE__"
            ]
        }, 
        "linker": {
            "librarypaths": [
                "$(OPENCV_LIB)"
            ], 
            "libraries": [
                "smartTilerStitcher"
            ], 
            "options": "-luuid"
        }
    }, 
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/aie",
 		"LIB_DIR/L1/include/aie-ml",
		"LIB_DIR/L1/include/aie-ml/dnn"
            ]
        }
    }, 
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/aie-ml/rgba2yuv/8bit_aie_8bit_gmio/host.cpp", 
                    "pl-freq": 400, 
                    "workdir": "./Work", 
                    "name": "libadf.a", 
                    "files": [
                        "xf_rgba2yuv.cc"
                    ]
                }
            ]
        }
    ], 
    "containers": [
        {
            "frequency": "400", 
            "name": "kernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": {
                    "vitis_aie_sim": 50, 
                    "vitis_aie_x86sim": 50, 
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 200, 
                    "vitis_hw_run": 30
                }
            }
        ], 
        "targets": [
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run",
            "vitis_aie_x86sim",
            "vitis_aie_sim"
        ], 
        "category": "canary"
    },
    "output_files": [
        "pl_sample_counts",
        "ISS_RPC_SERVER_PORT"
    ]    
}
