{
    "name": "xilinx remap_NPPC2_8UC3_8UC3_URAM_INTERPOLATION_NN l2 test",
    "description": [
        "Remap L2 function example"
    ],
    "flow": "system",
    "platform_allowlist": [
        "zcu104",
        "vck190",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "u280",
        "zcu102",
        "u250"
    ],
    "platform_properties": {
        "aws-vu9p-f1": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(OPENCV_INCLUDE)"
                    ]
                },
                "linker": {
                    "librarypaths": [
                        "$(OPENCV_LIB)"
                    ]
                }
            }
        },
        "zcu104": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include",
                        "$(SYSROOT)/usr/include/opencv4"
                    ]
                },
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib",
                        "${SYSROOT}/lib"
                    ]
                },
                "compiler_x86": {
                    "includepaths": [
                        "$(OPENCV_INCLUDE)"
                    ]
                },
                "linker_x86": {
                    "librarypaths": [
                        "$(OPENCV_LIB)"
                    ]
                }
            }
        },
        "vck190": {
            "host": {
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include",
                        "$(SYSROOT)/usr/include/opencv4"
                    ]
                },
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib",
                        "${SYSROOT}/lib"
                    ]
                },
                "compiler_x86": {
                    "includepaths": [
                        "$(OPENCV_INCLUDE)"
                    ]
                },
                "linker_x86": {
                    "librarypaths": [
                        "$(OPENCV_LIB)"
                    ]
                }
            }
        }
    },
    "gui": true,
    "launch": [
        {
            "target": "sw_emu",
            "cmd_args": " LIB_DIR/data/HD.jpg",
            "name": "sw_emu",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        },
        {
            "target": "hw_emu",
            "cmd_args": " LIB_DIR/data/HD.jpg",
            "name": "hw_emu",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        },
        {
            "target": "hw",
            "cmd_args": " LIB_DIR/data/HD.jpg",
            "name": "hardware_build",
            "ld_library_path": [
                "$(OPENCV_LIB)",
                "$(LD_LIBRARY_PATH)",
                "$(XILINX_XRT)/lib"
            ]
        }
    ],
    "data": [
        "LIB_DIR/data/HD.jpg",
        "LIB_DIR/data/HD.jpg"
    ],
    "host": {
        "host_exe": "remap",
        "compiler": {
            "sources": [
                "LIB_DIR/L2/examples/remap/xf_remap_tb.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/tests/remap/remap_NPPC2_8UC3_8UC3_URAM_INTERPOLATION_NN"
            ],
            "options": "-O3 -I$(XILINX_HLS)/include -I$(XILINX_HLS)/include -I$(XILINX_HLS)/include"
        }
    },
    "v++": {
        "compiler": {
            "symbols": [],
            "includepaths": [
                "LIB_DIR/L2/tests/remap/remap_NPPC2_8UC3_8UC3_URAM_INTERPOLATION_NN"
            ],
            "clflags": []
        }
    },
    "containers": [
        {
            "name": "krnl_remap",
            "accelerators": [
                {
                    "name": "remap_accel",
                    "location": "LIB_DIR/L2/examples/remap/xf_remap_accel.cpp",
                    "frequency": 300.0
                }
            ],
            "frequency": 300.0
        }
    ],
    "check_env": [
        {
            "env": "OPENCV_INCLUDE",
            "platform": [
                "u200",
                "u50",
                "aws-vu9p-f1"
            ],
            "echo": "Cannot find OpenCV include path. Please set OPENCV_INCLUDE variable"
        },
        {
            "env": "OPENCV_LIB",
            "platform": [
                "u200",
                "u50",
                "aws-vu9p-f1"
            ],
            "echo": "Cannot find Opencv lib path. Please set OPENCV_LIB variable"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "full"
    }
}