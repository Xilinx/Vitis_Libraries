{
    "name": "Xilinx GQE Filter L3 Test",
    "description": "Xilinx GQE Filter L3 Test",
    "flow": "system",
    "gui": false,
    "platform_type": "pcie",
    "platform_allowlist": [
        "u50"
    ],
    "platform_blocklist": [
        "other"
    ],
    "launch": [
        {
            "name": "generic launch for hw_emu flows",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/gqe3in1.xclbin -in PROJECT/db_data/ -scale 10000"
        },
        {
            "name": "generic launch for hw flows",
            "target": "hw",
            "cmd_args": " -xclbin BUILD/gqe3in1.xclbin -in PROJECT/db_data/"
        }
    ],
    "host": {
        "host_exe": "test_gqe_filter.exe",
        "compiler": {
            "sources": [
                "host/test_q5s.cpp",
                "LIB_DIR/L3/src/sw/gqe_table.cpp",
                "LIB_DIR/L3/src/sw/gqe_input.cpp",
                "LIB_DIR/L3/src/sw/gqe_workshop.cpp",
                "LIB_DIR/L3/src/sw/gqe_platform.cpp",
                "LIB_DIR/L3/src/sw/gqe_worker.cpp",
                "LIB_DIR/L3/src/sw/gqe_memcpy.cpp",
                "LIB_DIR/L3/src/sw/gqe_base_config.cpp",
                "LIB_DIR/L3/src/sw/gqe_partjoin_config.cpp",
                "LIB_DIR/L3/src/sw/gqe_join_config.cpp",
                "LIB_DIR/L3/src/sw/gqe_bloomfilter_config.cpp"
            ],
            "includepaths": [
                "host",
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/L1/include/sw",
                "LIB_DIR/L2/include/sw",
                "LIB_DIR/L3/tests/gqe/common",
                "LIB_DIR/L3/include/sw",
                "LIB_DIR/../utils/L1/include"
            ],
            "options": "-O3 -g -D XDEVICE=$(PLATFORM) -Wno-narrowing"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/L2/include/hw",
                "LIB_DIR/../utils/L1/include"
            ],
            "clflags": [
                "-g --config PROJECT/hls_pre.cfg -D PART_FLOW=0"
            ],
            "hls_jobs": 16
        },
        "linker": {
            "synth_jobs": 16,
            "impl_jobs": 16
        }
    },
    "containers": [
        {
            "name": "gqe3in1",
            "accelerators": [
                {
                    "name": "gqeKernel",
                    "location": "LIB_DIR/L2/src/hw/gqe_kernel_3_in_1.cpp",
                    "frequency": 300,
                    "num_compute_units": 1
                }
            ],
            "frequency": 200,
            "ldclflags": "--config PROJECT/conn_u50.cfg --config PROJECT/opts.cfg"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 81920,
                    "vitis_hw_emu": 28672,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 2400,
                    "vitis_hw_emu": 1200,
                    "vitis_hw_run": 15
                }
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
