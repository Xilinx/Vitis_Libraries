{
    "name": "RocksDB Compaction Acceleration",
    "description": "RocksDB compaction comparison acceleration",
    "flow": "vitis",
    "gui": false,
    "type": "application",
    "platform_type": "pcie",
    "platform_allowlist": [
        "u200"
    ],
    "platform_blocklist": [
        "other"
    ],
    "default_device": ["xilinx_u200_gen3x16_xdma_2_202110_1"
    ],
    "pre_build": [
        {
            "build_cmd": "${CXX} -std=c++11 -I LIB_DIR/L2/include/hw PROJECT/data_gen.cc -o data_gen; PROJECT/data_gen"
        }
    ],
    "host": {
        "host_exe": "test_compaction.exe",
        "compiler": {
            "sources": [
                "host/test_compaction.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/include/hw",
                "host"
            ],
            "options": "-O3 -std=c++11"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include/hw"
            ],
            "hls_jobs": 16,
            "clflags": [
                "-g"
            ]
        },
        "linker": {
            "synth_jobs": 16,
            "impl_jobs": 16
        }
    },
    "containers": [
        {
            "name": "compaction_test",
            "accelerators": [
                {
                    "name": "CUCoreLoopTop",
                    "location": "LIB_DIR/L2/src/hw/compaction_core.cc",
                    "frequency": 250,
                    "num_compute_units": 1
                }
            ],
            "frequency": 150,
            "ldclflags": " --config PROJECT/conn_u200.cfg --xp vivado_param:project.writeIntermediateCheckpoints=1"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 81920,
                    "vitis_hw_emu": 28672,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 1200,
                    "vitis_hw_emu": 720,
                    "vitis_hw_run": 15
                }
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
