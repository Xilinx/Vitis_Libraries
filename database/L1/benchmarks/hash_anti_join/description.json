{
    "name": "Xilinx Hash Anti Join Benchmark",
    "description": "Xilinx Hash Anti Join Benchmark",
    "flow": "system",
    "gui": true,
    "platform_type": "pcie",
    "platform_allowlist": [
        "u50"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u50": {
            "containers": [
                {
                    "name": "hash_anti_join",
                    "ldclflags": "--config PROJECT/conn_u50.cfg"
                }
            ]
        }
    },
    "launch": [
        {
            "name": "generic launch for hw_emu flows",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/hash_anti_join.xclbin -scale 10000"
        },
        {
            "name": "generic launch for hw flows",
            "target": "hw",
            "cmd_args": " -xclbin BUILD/hash_anti_join.xclbin"
        }
    ],
    "host": {
        "host_exe": "test_join.exe",
        "compiler": {
            "sources": [
                "host/test_join.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "host",
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include",
                "kernel"
            ],
            "options": "-O3 ",
            "symbols": [
                "USE_HBM"
            ]
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "kernel",
                "host"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/join_kernel.cpp",
                    "frequency": 200,
                    "clflags": " -D KERNEL_NAME=join_kernel",
                    "name": "join_kernel"
                }
            ],
            "frequency": 200,
            "name": "hash_anti_join"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 51200,
                    "vitis_hw_emu": 28672,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 720,
                    "vitis_hw_emu": 470,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
