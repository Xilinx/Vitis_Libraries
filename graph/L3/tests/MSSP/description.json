{
    "gui": false,
    "name": "Multi Source Shortest Path L3 Test", 
    "description": "An example for Single Source Shortest Path is based on Bellman-Ford algorithm equiped with a First-In-First-Out queue, without predecessor output", 
    "flow": "system",
    "xrm": true, 
    "platform_allowlist": [
        "u50"
    ], 
    "platform_blocklist": [
    ], 
    "platform_properties": {
        "u50": {
	        "v++": {
	            "compiler": {
		        "clflags": [
		            "--config PROJECT/conn_u50.cfg"
		            ]
		        }
	        }
	    }
    },
    "pre_build": [
        {
           "build_cmd":"make -f LIB_DIR/L3/lib/Makefile libgraphL3 TARGET=$(TARGET) CUR_DIR=PROJECT/BUILD"
        }
    ],
    "output_files": [
        "BUILD/libgraphL3.so",
        "BUILD/libgraphL3.json"
    ],
    "launch": [
        {
            "cmd_args": "-xclbin PROJECT/BUILD/shortestPath_top.xclbin -dataSetDir PROJECT/data/ -refDir PROJECT/data/", 
            "ld_library_path": ["BUILD",
                                "$(XILINX_XRT)/lib",
                                "$(XILINX_XRM)/lib"
                                ], 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L3/tests/MSSP/test_mssp.cpp" 
            ], 
            "includepaths": [
                "LIB_DIR/L3/include"
            ], 
            "options": "-I$(XILINX_XRM)/include"
        },
        "linker": {
            "librarypaths": [
                "BUILD"
            ], 
            "libraries" : [
                "graphL3"
            ],
            "options": "-L$(XILINX_XRM)/lib -lxrm "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/tests/shortest_path_float_pred/kernel",
                "LIB_DIR/../database/L1/include/hw",
                "LIB_DIR/../quantitative_finance/L1/include",
		        "LIB_DIR/../utils/L1/include"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/shortest_path_float_pred/kernel/shortestPath_top.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=shortestPath_top", 
                    "name": "shortestPath_top"
                }
            ], 
            "frequency": 300.0, 
            "name": "shortestPath_top"
        }
    ], 
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 470, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 30
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu",
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
