{
    "gui": false,
    "name": "All Pair Sparse Matrix K-Nearest Neighbors ", 
    "description": "An example for computing K-Nearest Neighbors of all pair vectors in a sparse graph", 
    "flow": "system", 
    "xrm": true,
    "platform_allowlist": [
        "u50"
    ], 
    "platform_blocklist": [], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u50_u280.cfg"
                    ]
                }
            }
        }
    },
    "pre_build": [
        {
           "build_cmd":"make -f LIB_DIR/L3/lib/Makefile libgraphL3 TARGET=$(TARGET) CUR_DIR=PROJECT/BUILD"
        }
    ],
    "output_files": [
        "BUILD/libgraphL3.so",
        "BUILD/libgraphL3.json"
    ],
    "launch": [
        {
            "cmd_args": "-xclbin PROJECT/BUILD/sparseSimilarityKernel.xclbin -offset PROJECT/data/knn_sparse_offset.csr -indiceWeight PROJECT/data/knn_sparse_indice_weight.csr -golden PROJECT/data/knn_sparse.mtx -topK 32", 
            "ld_library_path": ["BUILD",
                                "$(XILINX_XRT)/lib",
                                "$(XILINX_XRM)/lib"
                                ], 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L3/tests/knnSimilarityAPSparse/test_knnSimilarityAPSparse.cpp" 
            ], 
            "includepaths": [
                "LIB_DIR/L3/include",
                "LIB_DIR/../utils/L1/iclude"
            ], 
            "options": "-I$(XILINX_XRM)/include"
        }, 
        "linker": {
            "librarypaths": [
                "BUILD"
            ], 
            "libraries": [
                "graphL3"
            ], 
            "options": "-L$(XILINX_XRM)/lib -lxrm "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw", 
                "LIB_DIR/L2/tests/sparse_similarity/kernel"
            ]
        } 
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/sparse_similarity/kernel/sparseSimilarityKernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=sparseSimilarityKernel --hls.pre_tcl $(CUR_DIR)/cfg_hls.tcl", 
                    "name": "sparseSimilarityKernel", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "sparseSimilarityKernel", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "config", 
                                    "memory": "HBM[24]"
                                }, 
                                {
                                    "name": "sourceIndice", 
                                    "memory": "HBM[24]"
                                }, 
                                {
                                    "name": "sourceWeight", 
                                    "memory": "HBM[24]"
                                }, 
                                {
                                    "name": "resultID", 
                                    "memory": "HBM[24]"
                                }, 
                                {
                                    "name": "similarity", 
                                    "memory": "HBM[24]"
                                }, 
                                {
                                    "name": "offsetCSR0", 
                                    "memory": "HBM[0]"
                                }, 
                                {
                                    "name": "indiceCSR0", 
                                    "memory": "HBM[1]"
                                }, 
                                {
                                    "name": "weight0", 
                                    "memory": "HBM[2]"
                                }, 
                                {
                                    "name": "offsetCSR1", 
                                    "memory": "HBM[3]"
                                }, 
                                {
                                    "name": "indiceCSR1", 
                                    "memory": "HBM[4]"
                                }, 
                                {
                                    "name": "weight1", 
                                    "memory": "HBM[5]"
                                }, 
                                {
                                    "name": "offsetCSR2", 
                                    "memory": "HBM[6]"
                                }, 
                                {
                                    "name": "indiceCSR2", 
                                    "memory": "HBM[7]"
                                }, 
                                {
                                    "name": "weight2", 
                                    "memory": "HBM[8]"
                                }, 
                                {
                                    "name": "offsetCSR3", 
                                    "memory": "HBM[9]"
                                }, 
                                {
                                    "name": "dindiceCSR3", 
                                    "memory": "HBM[10]"
                                }, 
                                {
                                    "name": "weight3", 
                                    "memory": "HBM[11]"
                                }, 
                                {
                                    "name": "offsetCSR4", 
                                    "memory": "HBM[12]"
                                }, 
                                {
                                    "name": "indiceCSR4", 
                                    "memory": "HBM[13]"
                                }, 
                                {
                                    "name": "weight4", 
                                    "memory": "HBM[14]"
                                }, 
                                {
                                    "name": "offsetCSR5", 
                                    "memory": "HBM[15]"
                                }, 
                                {
                                    "name": "indiceCSR5", 
                                    "memory": "HBM[16]"
                                }, 
                                {
                                    "name": "weight5", 
                                    "memory": "HBM[17]"
                                }, 
                                {
                                    "name": "offsetCSR6", 
                                    "memory": "HBM[18]"
                                }, 
                                {
                                    "name": "indiceCSR6", 
                                    "memory": "HBM[19]"
                                }, 
                                {
                                    "name": "weight6", 
                                    "memory": "HBM[20]"
                                }, 
                                {
                                    "name": "offsetCSR7", 
                                    "memory": "HBM[21]"
                                }, 
                                {
                                    "name": "dindiceCSR7", 
                                    "memory": "HBM[22]"
                                }, 
                                {
                                    "name": "weight7", 
                                    "memory": "HBM[23]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "sparseSimilarityKernel"
        }
    ], 
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 51200, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 800, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu",
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
