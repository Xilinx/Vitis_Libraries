{
    "gui": false,
    "name": "Label Propagation L3 Test", 
    "description": "", 
    "flow": "system",
    "xrm": true, 
    "platform_allowlist": [
        "u50"
    ], 
    "platform_blocklist": [], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        }
    },
    "pre_build": [
        {
           "build_cmd":"make -f LIB_DIR/L3/lib/Makefile libgraphL3 TARGET=$(TARGET) CUR_DIR=PROJECT/BUILD"
        }
    ],
    "output_files": [
        "BUILD/libgraphL3.so",
        "BUILD/libgraphL3.json"
    ],
    "launch": [
        {
            "cmd_args": "-xclbin PROJECT/BUILD/LPKernel.xclbin -offset PROJECT/data/csr_offsets.txt -indice PROJECT/data/csr_columns.txt -golden PROJECT/data/label.txt", 
            "ld_library_path": ["BUILD",
                                "$(XILINX_XRT)/lib",
                                "$(XILINX_XRM)/lib"
                                ], 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L3/tests/labelPropagation/test_labelpropagation.cpp" 
            ], 
            "includepaths": [
                "LIB_DIR/L3/include"
            ], 
            "options": "-I$(XILINX_XRM)/include"
        }, 
        "linker": {
            "librarypaths": [
                "BUILD"
            ], 
            "libraries": [
                "graphL3"
            ], 
            "options": "-L$(XILINX_XRM)/lib -lxrm "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/label_propagation/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/label_propagation/kernel/label_propagation_top.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=LPKernel", 
                    "name": "LPKernel", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "LPKernel", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "offsetCSR", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "indexCSR", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "offsetCSC", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "indexCSC", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "indexCSC2", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "pingHashBuf", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "pongHashBuf", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "labelPing", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "labelPong", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "LPKernel"
        }
    ], 
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 950, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu",
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
