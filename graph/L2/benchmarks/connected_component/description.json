{
    "gui": false,
    "name": "Xilinx Connected Component Benchmark",
    "description": "In graph theory, a component, sometimes called a connected component, of an undirected graph is a subgraph in which any two vertices are connected to each other by paths, and which is connected to no additional vertices in the supergraph (from wikipedia)",
    "flow": "system",
    "platform_allowlist": [
        "u250",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "aws-vu9p-f1": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        },
        "u250": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        }
    },
    "data": [
        "PROJECT/data/test_offset.csr",
        "PROJECT/data/test_column.csr",
        "PROJECT/data/test_golden.mtx"
    ],
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/wcc_kernel.xclbin -o PROJECT/data/test_offset.csr -c PROJECT/data/test_column.csr -g PROJECT/data/test_golden.mtx",
            "name": "generic launch for all flows"
        }
    ],
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "host/main.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/benchmarks/connected_component/host",
                "LIB_DIR/L2/benchmarks/connected_component/kernel",
                "LIB_DIR/ext/xcl2"
            ],
            "options": "-O3 "
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/benchmarks/connected_component/kernel"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/wcc_kernel.cpp",
                    "frequency": 300.0,
                    "clflags": " -D KERNEL_NAME=wcc_kernel",
                    "name": "wcc_kernel",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "wcc_kernel",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "columnG1",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "offsetG1",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "column512G2",
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "column32G2",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "offsetG2",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "offsetG2Tmp1",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "offsetG2Tmp2",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "queue512",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "queue",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "result512",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "result32",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 300.0,
            "name": "wcc_kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 49152,
                    "vitis_hw_emu": 32768,
                    "vitis_sw_emu": 32768,
                    "vitis_hw_run": 32768
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}