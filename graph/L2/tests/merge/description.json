{
    "gui": false,
    "name": "Xilinx Merge Kernel",
    "description": "merge in graph cases",
    "flow": "system",
    "platform_allowlist": [
        "u50"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u50.cfg --advanced.param compiler.userPostSysLinkOverlayTcl=PROJECT/postSysLink.tcl"
                    ]
                }
            }
        }
    },
    "data": [
        "PROJECT/merge_data/example"
    ],
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/merge_kernel.xclbin -io PROJECT/merge_data/example/exapmle-wt_offset.mtx -ie PROJECT/merge_data/example/exapmle-wt_edge.mtx -iw PROJECT/merge_data/example/exapmle-wt_weight.mtx -ic PROJECT/merge_data/example/exapmle-wt_c.mtx -oo PROJECT/merge_data/example/exapmle-wt_offset_out.mtx -oe PROJECT/merge_data/example/exapmle-wt_edge_out.mtx -ow PROJECT/merge_data/example/exapmle-wt_weight_out.mtx -go PROJECT/merge_golden/example/exapmle-wt_offset_out.mtx -ge PROJECT/merge_golden/example/exapmle-wt_edge_out.mtx -gw PROJECT/merge_golden/example/exapmle-wt_weight_out.mtx",
            "name": "generic launch for all flows"
        }
    ],
    "host": {
        "host_exe": "host.exe",
        "linker": {
            "options": "-lgomp"
        },
        "compiler": {
            "sources": [
                "host/test_merge.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/include/hw",
                "LIB_DIR/L2/tests/merge/host",
                "LIB_DIR/L2/tests/merge/kernel",
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ],
            "options": "-O3 -std=c++11"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/include/hw",
                "LIB_DIR/L2/tests/merge/host",
                "LIB_DIR/L2/tests/merge/kernel"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/merge_kernel.cpp",
                    "frequency": 300.0,
                    "clflags": " -D KERNEL_NAME=merge_kernel",
                    "name": "merge_kernel",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "merge_kernel",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "m_axi_gmem0",
                                    "memory": "HBM[0]"
                                },
                                {
                                    "name": "m_axi_gmem1",
                                    "memory": "HBM[1]"
                                },
                                {
                                    "name": "m_axi_gmem2",
                                    "memory": "HBM[2:3]"
                                },
                                {
                                    "name": "m_axi_gmem3",
                                    "memory": "HBM[4:5]"
                                },
                                {
                                    "name": "m_axi_gmem4",
                                    "memory": "HBM[6]"
                                },
                                {
                                    "name": "m_axi_gmem5",
                                    "memory": "HBM[7]"
                                },
                                {
                                    "name": "m_axi_gmem6",
                                    "memory": "HBM[8:9]"
                                },
                                {
                                    "name": "m_axi_gmem7",
                                    "memory": "HBM[10:11]"
                                },
                                {
                                    "name": "m_axi_gmem8",
                                    "memory": "HBM[12]"
                                },
                                {
                                    "name": "m_axi_gmem9",
                                    "memory": "HBM[13]"
                                },
                                {
                                    "name": "m_axi_gmem10",
                                    "memory": "HBM[14]"
                                },
                                {
                                    "name": "m_axi_gmem11",
                                    "memory": "HBM[15]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 300.0,
            "name": "merge_kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
		    "vitis_hw_build": 49152,
		    "vitis_hw_emu": 32768,
		    "vitis_sw_emu": 32768,
		    "vitis_hw_run": 32768
		},
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
