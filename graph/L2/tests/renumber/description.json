{
    "gui": true, 
    "name": "Xilinx Renumber Test", 
    "description": "An example for fast renumber in graph cases", 
    "flow": "system", 
    "platform_allowlist": [
        "u50" 
    ], 
    "platform_blocklist": [
        "zc"
    ], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        } 
    },
    "data":[
        "PROJECT/data/as-Skitter.txt",
        "PROJECT/data/as-Skitter-red.txt"
    ],
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/kernel_renumber.xclbin -i PROJECT/data/as-Skitter.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "linker": {
            "options": "-lgomp"
        },
        "compiler": {
            "sources": [
                "host/test_renumber.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/renumber/host", 
                "LIB_DIR/L2/tests/renumber/kernel", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 -std=c++11"
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/renumber/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/kernel_renumber.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=kernel_renumber", 
                    "name": "kernel_renumber", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "kernel_renumber", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "m_axi_gmem0", 
                                    "memory": "HBM[8]"
                                }, 
                                {
                                    "name": "m_axi_gmem1", 
                                    "memory": "HBM[0:1]"
                                }, 
                                {
                                    "name": "m_axi_gmem2", 
                                    "memory": "HBM[2:3]"
                                }, 
                                {
                                    "name": "m_axi_gmem3", 
                                    "memory": "HBM[4:5]"
                                }, 
                                {
                                    "name": "m_axi_gmem4", 
                                    "memory": "HBM[6:7]"
                                } 
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "kernel_renumber"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
		    "vitis_hw_build": 49152,
		    "vitis_hw_emu": 32768,
		    "vitis_sw_emu": 32768,
		    "vitis_hw_run": 32768
		}, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
