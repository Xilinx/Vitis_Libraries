{
    "gui": false,
    "name": "Xilinx Calculate Degree Test",
    "description": "CalcuDgree is an algorithm used to calculate indegree for CSR format input or outdegree for CSC format input. The algorithm is quite easy, but due to DDR limits, we cannot get one data per clock, so in our design, we use a cache with depth ajustable to get much better performance.",
    "flow": "system",
    "platform_allowlist": [
        "u250",
        "vck190",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "aws-vu9p-f1": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        },
        "u250": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        },
        "vck190": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_vck190.cfg"
                    ]
                }
            }
        }
    },
    "data": [
        "PROJECT/data"
    ],
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/kernel_calcuDegree_0.xclbin -dataSetDir PROJECT/data/ -refDir PROJECT/data/",
            "name": "generic launch for all flows"
        },
        {
            "cmd_args": " -xclbin kernel_calcuDegree_0.xclbin",
            "platform": "vck190",
            "name": "vck 190 launch"
        },
        {
            "cmd_args": " -xclbin BUILD/kernel_calcuDegree_0.xclbin -dataSetDir PROJECT/data/ -refDir PROJECT/data/",
            "target": "sw_emu",
            "platform": "vck190",
            "name": "sw_emu for vck190 launch"
        }
    ],
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "host/test_calcuDegree.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/tests/calcu_degree/host",
                "LIB_DIR/L2/tests/calcu_degree/kernel",
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../database/L1/include/hw",
                "LIB_DIR/../quantitative_finance/L1/include",
                "LIB_DIR/../utils/L1/include"
            ],
            "options": "-O3 "
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include",
                "LIB_DIR/L2/tests/calcu_degree/kernel",
                "LIB_DIR/../database/L1/include/hw",
                "LIB_DIR/../quantitative_finance/L1/include",
                "LIB_DIR/../utils/L1/include"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/kernel_calcuDegree_0.cpp",
                    "frequency": 300.0,
                    "clflags": " -D KERNEL0",
                    "name": "kernel_calcuDegree_0",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "kernel_calcuDegree_0",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "degreeCSR",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "indiceCSC",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 300.0,
            "name": "kernel_calcuDegree_0"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 32768,
                    "vitis_sw_emu": 32768,
                    "vitis_hw_run": 32768
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}