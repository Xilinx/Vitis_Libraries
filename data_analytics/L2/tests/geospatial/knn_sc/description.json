{
    "gui": false, 
    "name": "Xilinx L2 Geospatial KNN (SC) Test", 
    "description": "A SystemCompiler example for geospatial knn acceleration.", 
    "flow": "vitis", 
    "platform_allowlist": [
        "u50" 
    ], 
    "platform_blocklist": [
        "zc"
    ], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "clflags": []
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": "data/yellow_tripdata_schema.txt data/yellow_tripdata.csv -73 40 5 200", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "host/main.cpp",
		        "kernel/knn_acc.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw",
                "./host",
                "./kernel",
                "LIB_DIR/../graph/L1/include/hw"
            ],
            "options": "-O3"
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "./kernel",
                "LIB_DIR/../graph/L1/include/hw"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/knn_acc.cpp", 
                    "frequency": 300.0, 
                    "name": "kernel_knn"
                }
            ], 
            "frequency": 300.0, 
            "name": "kernel_knn"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
		    "vitis_hw_build": 51200,
		    "vitis_hw_emu": 32768,
		    "vitis_sw_emu": 32768,
		    "vitis_hw_run": 32768
		}, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
