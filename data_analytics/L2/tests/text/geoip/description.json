{
    "gui": false,
    "name": "Xilinx Geo IP Test",
    "description": "Geo information look up with IP",
    "flow": "system",
    "platform_type": "pcie",
    "platform_allowlist": [
        "u250",
        "vck190",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u200_u250.cfg"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/GeoIP_kernel.xclbin -csv PROJECT/test_geoip.csv",
            "name": "generic launch for all flows"
        }
    ],
    "data": [
        "PROJECT/test_geoip.csv"
    ],
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "host/main.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "host",
                "kernel",
                "LIB_DIR/L2/include/hw",
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ],
            "options": "-O3 "
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include/hw",
                "kernel"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/geoip_kernel.cpp",
                    "frequency": 300.0,
                    "clflags": " -D KERNEL_NAME=GeoIP_kernel",
                    "name": "GeoIP_kernel",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "GeoIP_kernel",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "ip",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "netHigh16",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "netHigh16ID",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "netLow21",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "net2Low21",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "netID",
                                    "memory": "DDR[0]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 300.0,
            "name": "GeoIP_kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 300,
                    "vitis_hw_emu": 600,
                    "vitis_sw_emu": 300,
                    "vitis_hw_run": 300
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}