{
    "gui": false,
    "name": "Xilinx Two Gram Predicate Test",
    "description": "2-Gram Predicate based on inverted index",
    "flow": "system",
    "platform_type": "pcie",
    "platform_allowlist": [
        "u250",
        "aws-vu9p-f1"
    ],
    "platform_blocklist": [
        "zc"
    ],
    "platform_properties": {
        "u250": {
            "v++": {
                "compiler": {
                    "clflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/TGP_Kernel.xclbin -in PROJECT/data/test.txt -golden PROJECT/data/golden.txt",
            "name": "generic launch for all flows"
        }
    ],
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "host/main.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "host",
                "kernel",
                "LIB_DIR/L2/include/hw",
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ],
            "options": "-O3 "
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include/hw",
                "kernel"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/predicate_kernel.cpp",
                    "frequency": 250.0,
                    "name": "TGP_Kernel",
                    "num_compute_units": 1,
                    "compute_units": [
                        {
                            "name": "TGP_Kernel",
                            "slr": "SLR0",
                            "arguments": [
                                {
                                    "name": "field",
                                    "memory": "HBM[0]"
                                },
                                {
                                    "name": "offset",
                                    "memory": "HBM[1]"
                                },
                                {
                                    "name": "idfValue",
                                    "memory": "HBM[2]"
                                },
                                {
                                    "name": "tfAddr",
                                    "memory": "HBM[3]"
                                },
                                {
                                    "name": "tfValue",
                                    "memory": "HBM[4]"
                                },
                                {
                                    "name": "indexID",
                                    "memory": "HBM[5]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 250.0,
            "name": "TGP_Kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 51200,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 1200,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 300,
                    "vitis_hw_run": 300
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}