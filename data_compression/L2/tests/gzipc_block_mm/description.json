{
    "name": "Xilinx GZip 32KB Compression",
    "description": [
        "Xilinx Gzip Compression with 32 KB block size"
    ],
    "flow": "system",
    "launch": [
        {
            "cmd_args": "-xbin BUILD/compress.xclbin -c PROJECT/data/sample.txt",
            "name": "generic launch for all flows"
        }
    ],
    "post_launch": [
        {
            "target": "sw_emu",
            "launch_cmd": [
                "XCL_EMULATION_MODE=$(TARGET) ./run.sh HOST_EXE LIB_DIR BUILD/compress.xclbin"
            ]
        },
        {
            "target": "hw_emu",
            "launch_cmd": [
                "XCL_EMULATION_MODE=$(TARGET) ./run.sh HOST_EXE LIB_DIR BUILD/compress.xclbin"
            ]
        },
        {
            "target": "hw",
            "launch_cmd": [
                "./run.sh HOST_EXE LIB_DIR BUILD/compress.xclbin"
            ]
        }
    ],
    "config_make": "./config.mk",
    "default_device": "u250",
    "platform_blocklist": [
        "zc"
    ],
    "platform_allowlist": [
        "u250",
        "vck190",
        "aws-vu9p-f1"
    ],
    "data": [
        "PROJECT/data/sample.txt"
    ],
    "host": {
        "host_exe": "xil_gzip",
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/common/libs/compress/gzipOCLHost.cpp",
                "LIB_DIR/common/libs/compress/gzipBase.cpp",
                "LIB_DIR/common/libs/compress/gzipApp.cpp",
                "LIB_DIR/common/libs/compress/compressApp.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/zlib-1.2.7/crc32.c",
                "LIB_DIR/common/thirdParty/zlib-1.2.7/adler32.c"
            ],
            "includepaths": [
                "LIB_DIR/L2/tests/src/",
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/common/libs/compress/",
                "LIB_DIR/common/thirdParty/zlib-1.2.7"
            ],
            "symbols": [
                "GZIP_MULTICORE_COMPRESS",
                "GZIP_MODE"
            ]
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/../security/L1/include"
            ]
        }
    },
    "containers": [
        {
            "name": "compress",
            "accelerators": [
                {
                    "name": "xilGzipCompBlock",
                    "location": "LIB_DIR/L2/src/gzip_compress_multicore_mm.cpp"
                }
            ]
        }
    ],
    "output_files": [
        "data/sample.txt.*",
        "sample_run.*",
        "test.list"
    ],
    "testinfo": {
        "disable": false,
        "stacksize": 16384,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 25,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    },
    "platform_properties": {
        "vck190": {
            "containers": [
                {
                    "name": "compress",
                    "ldclflags": "--clock.id 4:xilGzipCompBlock_1"
                }
            ]
        }
    },
    "gui": true
}