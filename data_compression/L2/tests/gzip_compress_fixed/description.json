{
    "name": "Xilinx GZIP Fixed Compress", 
    "description": [
        "Xilinx GZIP Compression with fixed huffman encoding."
    ], 
    "flow": "vitis", 
    "launch": [
        {
            "cmd_args": "-cx BUILD/compress_streaming.xclbin -c PROJECT/sample.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "config_make": "./config.mk", 
    "platform_blacklist": [
        "zc", 
        "vck"
    ], 
    "platform_whitelist": [
        "u200", 
        "u250", 
        "u50"
    ], 
    "host": {
        "host_exe": "xil_gzip", 
        "compiler": {
            "sources": [
                "./src/host.cpp", 
                "LIB_DIR/L2/tests/src/gzip_compress_streaming.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp", 
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp", 
                "LIB_DIR/common/libs/logger/logger.cpp", 
                "LIB_DIR/common/thirdParty/zlib-1.2.7/crc32.c"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/src/", 
                "LIB_DIR/common/thirdParty/zlib-1.2.7"
            ], 
            "symbols": [
                "PARALLEL_BLOCK=8"
            ]
        }
    }, 
    "v++": {
        "build_datafiles": [
            "PROJECT/advanced.cfg"
        ], 
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=8"
            ], 
            "clflags": [
                "--config PROJECT/advanced.cfg"
            ],
            "includepaths":[
                 "LIB_DIR/../security/L1/include"
             ]
        }
    }, 
    "containers": [
        {
            "name": "compress_streaming", 
            "ldclflags": "--config PROJECT/connectivity.ini", 
            "accelerators": [
                {
                    "name": "xilCompDatamover", 
                    "location": "LIB_DIR/L2/tests/src/gzip_block_comp_dm.cpp"
                },
                {
                    "name": "xilGzipCompressFixedStreaming", 
                    "location": "LIB_DIR/L2/src/gzip_compress_fixed_stream.cpp"
                }
            ]
        }
    ], 
    "output_files": "sample.txt.*", 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 25, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
