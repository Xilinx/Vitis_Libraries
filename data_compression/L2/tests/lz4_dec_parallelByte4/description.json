{
    "name": "Xilinx LZ4 Decompress Parallel Byte 4",
    "description": [
        "Xilinx LZ4 Decompress Parallel Byte 4"
    ],
    "flow": "vitis",
    "launch": [
        {
            "cmd_args": "-dx BUILD/decompress.xclbin -d PROJECT/sample.txt.lz4",
            "name": "generic launch for all flows"
        }
    ],
    "host": {
        "host_exe": "xil_lz4",
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/L2/tests/src/lz4.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/xxhash/xxhash.c"
            ],
            "includepaths": [
                "LIB_DIR/L2/include"
            ],
            "symbols": [
                "PARALLEL_BLOCK=1",
                "HOST_BUFFER_SIZE_IN_MB=128"
            ]
        }
    },
    "v++": {
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=1",
                "PARALLEL_BYTE=4"
            ]
        }
    },
    "containers": [
        {
            "name": "decompress",
            "ldclflags": "--profile_kernel data:all:all:all --vivado.prop run.impl_1.STEPS.ROUTE_DESIGN.TCL.POST=PROJECT/post_route.tcl",
            "accelerators": [
                {
                    "name": "xilLz4Decompress",
                    "location": "LIB_DIR/L2/src/lz4_multibyte_decompress_mm.cpp"
                }
            ]
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
