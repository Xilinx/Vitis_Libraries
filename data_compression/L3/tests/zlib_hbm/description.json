{
    "name": "ZLIB Application for HBM",
    "description": [
        "ZLIB application for HBM resides in `L3/tests/zlib_hbm` directory."
    ],
    "flow": "vitis",
    "platform_blacklist": [
        "u200",
        "u250"
    ],
    "launch": [
        {
            "cmd_args": "-sx BUILD/compress_decompress.xclbin -v PROJECT/sample.txt",
            "name": "generic launch for all flows"
        }
