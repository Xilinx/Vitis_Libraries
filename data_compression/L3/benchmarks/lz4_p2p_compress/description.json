{
    "name": "Xilinx LZ4 P2P Application For Compression",
    "description": [
        "LZ4_P2P application for Compression resides in `L3/benchmarks/lz4_p2p_compress` directory."
    ],
    "flow": "system",
    "default_device": "samsung_u2",
    "platform_blocklist": [
        "u50",
        "u280",
        "zc",
        "vck"
    ],
    "platform_allowlist": [
        "u250",
        "samsung_u2",
        "samsung_U2"
    ],
    "launch": [
        {
            "cmd_args": "-cx BUILD/compress.xclbin -c PROJECT/sample.txt",
            "name": "generic launch for all flows"
        }
    ],
    "post_launch": [
        {
            "launch_cmd": [
                "XCL_EMULATION_MODE=$(TARGET) ./run.sh HOST_EXE LIB_DIR BUILD/compress.xclbin"
            ]
        }
    ],
    "config_make": "./config.mk",
    "host": {
        "host_exe": "xil_lz4",
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/L3/src/lz4_p2p_comp.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/xxhash/xxhash.c"
            ],
            "includepaths": [
                "LIB_DIR/L3/include",
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/common/thirdParty/xxhash"
            ],
            "symbols": [
                "PARALLEL_BLOCK=8",
                "SINGLE_XCLBIN=false"
            ]
        }
    },
    "v++": {
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=8",
                "C_COMPUTE_UNITS=2"
            ]
        }
    },
    "containers": [
        {
            "name": "compress",
            "accelerators": [
                {
                    "name": "xilLz4Compress",
                    "location": "LIB_DIR/L2/src/lz4_compress_mm.cpp",
                    "num_compute_units": 2
                },
                {
                    "name": "xilLz4Packer",
                    "location": "LIB_DIR/L2/src/lz4_packer_mm.cpp",
                    "num_compute_units": 2
                }
            ]
        }
    ],
    "output_files": [
        "sample.txt.*"
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 25,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu"
        ],
        "category": "canary"
    },
    "gui": true
}