{
    "name": "LZ4 Application",
    "description": [
        "LZ4 Application resides in `L3/demos/lz4_app` directory."
    ],
    "flow": "vitis",
    "launch": [
        {
            "cmd_args": "-cx BUILD/compress.xclbin -c PROJECT/sample.txt",
            "name": "generic launch for all flows"
        }
    ],
    "post_launch": [
        {
            "script_name": "run.sh",
            "script_args": "HOST_EXE LIB_DIR BUILD/compress.xclbin BUILD/decompress.xclbin"
        }
    ],
    "config_make": "config.mk",
    "host": {
        "host_exe": "xil_lz4",
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/L3/src/lz4.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/xxhash/xxhash.c"
            ],
            "includepaths": [
                "LIB_DIR/L3/include",
                "LIB_DIR/L1/include/hw"
            ],
            "symbols": [
                "PARALLEL_BLOCK=8"
            ]
        }
    },
    "v++": {
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=8"
            ]
        }
    },
    "containers": [
        {
            "name": "compress",
            "ldclflags": "--profile_kernel data:all:all:all",
            "accelerators": [
                {
                    "name": "xilLz4Compress",
                    "location": "LIB_DIR/L2/src/lz4_compress_mm.cpp",
                    "num_compute_units": "2",
                    "compute_units": [
                        {
                            "arguments": [
                                {
                                    "name": "m_axi_gmem0",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "m_axi_gmem1",
                                    "memory": "DDR[0]"
                                }
                            ]
                        },
                        {
                            "arguments": [
                                {
                                    "name": "m_axi_gmem0",
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "m_axi_gmem1",
                                    "memory": "DDR[1]"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "name": "decompress",
            "ldclflags": "--profile_kernel data:all:all:all",
            "accelerators": [
                {
                    "name": "xilLz4Decompress",
                    "location": "LIB_DIR/L2/src/lz4_decompress_mm.cpp",
                    "num_compute_units": "2",
                    "compute_units": [
                        {
                            "arguments": [
                                {
                                    "name": "m_axi_gmem0",
                                    "memory": "DDR[0]"
                                },
                                {
                                    "name": "m_axi_gmem1",
                                    "memory": "DDR[0]"
                                }
                            ]
                        },
                        {
                            "arguments": [
                                {
                                    "name": "m_axi_gmem0",
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "m_axi_gmem1",
                                    "memory": "DDR[1]"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
