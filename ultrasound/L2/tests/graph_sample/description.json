{
    "name": "L2 graph sample Test",
    "description": "graph sample Test",
    "flow": "aie",
    "platform_allowlist": [
        "vck190"
    ],
    "platform_properties": {
        "vck190": {
            "v++": {
                "package": {
                    "clflags": [
                        "--package.defer_aie_run"
                    ]
                }
            }
        }
    },
    "gui": true,
    "data": [
        "PROJECT/data"
    ],
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/",
                "LIB_DIR/L2/include/"
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/graph_sample/aie_graph/graph.cpp",
                    "workdir": "./Work",
                    "name": "libadf.a",
                    "config": "aie.cfg"
                }
            ]
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "max_memory_MB": {
                    "vitis_aie_sim": 32768,
                    "vitis_aie_x86sim": 32768
                },
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_aie_sim",
            "vitis_aie_x86sim"
        ],
        "category": "canary"
    },
    "aie_types": [
        "AIE1"
    ]
}